private class Persona{
  String nombres;
  String apellidos;
  String identificacion;
  String nacionalidad;
  int edad;
  String deporte;

  public Persona(String nombres, String apellidos, String identificacion, String nacionalidad, int edad, String deporte, String tipo) {

    this.nombres = nombres;
    this.apellidos = apellidos;
    this.identificacion = identificacion;
    this.nacionalidad = nacionalidad;
    this.edad = edad;
    this.deporte = deporte;
}

@Override

public String toString (){
  return this.nombres;
}
